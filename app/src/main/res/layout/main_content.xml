<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="@dimen/list_bottom_nav_height"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.duan.musicoco.view.SheetListView
            android:id="@+id/my_sheet_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@android:color/transparent"
            android:dividerHeight="10dp"
            android:layoutAnimation="@anim/my_sheets_item_anim"
            android:paddingBottom="20dp"
            android:paddingTop="10dp"
            android:scrollbars="none" />

        <LinearLayout
            android:id="@+id/empty_list_notice_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="16dp"
            android:orientation="vertical" />
    </FrameLayout>


</android.support.v4.widget.NestedScrollView>